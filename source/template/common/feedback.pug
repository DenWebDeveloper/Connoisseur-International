#feedback.feedback
	.container  
		.row 
			.col-md-5.offset-md-1
				h2 Login
				form.form
					.form-group
						.input-group
							span.icon-user
							input.form-control(type='text', placeholder='Login')
						.input-group
							span.icon-password
							input.form-control(type='text', placeholder='Password')
					button.btn.btn-primary(type='submit') Login
			.col-md-5  
				h2 Contact us
				form.form
					.input-group
						span.icon-name
						input.form-control(type='text', placeholder='Name')
					.input-group
						span.icon-name-company
						input.form-control(type='text', placeholder='Company name')
					.input-group
						span.icon-mail
						input.form-control(type='text', placeholder='Your.mail@mail.box')
					.form-group
						label(for='your-message') Your message:
						textarea#your-message.form-control(rows='3', placeholder='Your message')
					button.btn.btn-primary(type='submit') Send
		.contact-us
			h2.pl-3 Connoisseur International Distribution
			.row.contact-us-wrapper
				.col-md-5.offset-md-1
					ul.contact-list
						li Connoisseur International Distribution Ltd
						li Unit 1 Beta Way
						li Thorpe Industrial Park
						li EGHAM
						li Surrey
						li TW20 8RE
						li Great Britain
					a(href='tel:+44(0)1895 438822') Tel: +44(0)1895 438822<br>
					a(href='#') Fax: +44(0)1895 438833
				.col-md-6.container-map
					iframe#map(src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2488.2006338531573!2d-0.5302126841754697!3d51.41774097962051!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x0!2zNTHCsDI1JzAzLjkiTiAwwrAzMSc0MC45Ilc!5e0!3m2!1suk!2sua!4v1490127691912") 